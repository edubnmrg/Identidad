}
    <div class="container-fluid" style="margin-left:2EM">
        <h1>Detalle Apoderado Club</h1>

        <div>
            <dl class="row">
                <dt class="col-sm-2 font2b">
                    @Html.DisplayNameFor(model => model.Club.NombreClub)
                </dt>
                <dd class="col-sm-10 font2">
                    @Html.DisplayFor(model => model.Club.NombreClub)
                </dd>
                <dt class="col-sm-2 font2b">
                    @Html.DisplayNameFor(model => model.Perfil.NombreCompleto)
