@model Identidad.Models.Recursos.CategoriaTorneo

@{
    ViewData["Title"] = "Crear Categoria Torneo";
}
    <div class="container-fluid" style="margin-left:3EM">

        <h1>Crear Categoria Torneo</h1>


        <div class="row font2">
            <div class="col-md-4">
                <form asp-action="Create">
                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                    <div class="form-group">
                        <label asp-for="catTorneo" class="control-label font2b"></label>
                        <input asp-for="catTorneo" class="form-control" />
                        <span asp-validation-for="catTorneo" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <input type="submit" value="Crear" class="btn btn-primary font2" />
                    </div>
                </form>
            </div>
        </div>

        <div>
            <a class="font2 " asp-action="Index">Volver a la Lista</a>
        </div>

    </div>
