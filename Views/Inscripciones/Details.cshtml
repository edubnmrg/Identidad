@model Identidad.Models.Inscripcion

@{
    ViewData["Title"] = "Detalle Inscrpciones";
}

    <h1>Detalle Inscrpciones</h1>

<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2 font2b">
            @Html.DisplayNameFor(model => model.Perfil)
        </dt>
        <dd class = "col-sm-10 font2">
            @Html.DisplayFor(model => model.Perfil.nombre)
        </dd>
        <dt class = "col-sm-2 font2b">
            @Html.DisplayNameFor(model => model.Torneo)
        </dt>
        <dd class = "col-sm-10 font2">
            @Html.DisplayFor(model => model.Torneo.NombreTorneo)
        </dd>
    </dl>
</div>
<div>
    <a class="font2 " asp-action="Edit" asp-route-id="@Model.InscripcionID">Editar</a> |
    <a class="font2 " asp-action="Index">Volver a la Lista</a>
</div>
