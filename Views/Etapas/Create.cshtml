@model Identidad.Models.Recursos.Etapa

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>Etapa</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="NombreEtapa" class="control-label font2b"></label>
                <input asp-for="NombreEtapa" class="form-control font2" />
                <span asp-validation-for="NombreEtapa" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Agregar" class="btn btn-primary font2" />
            </div>
        </form>
    </div>
</div>

<div>
    <a class="font2" asp-action="Index">Volver a la Lista</a>
</div>

