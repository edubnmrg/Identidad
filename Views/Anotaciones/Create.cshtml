@model Identidad.Models.Anotacion

@{
    ViewData["Title"] = "Agregar Inscripcion";
}

    <h1>Agregar Inscripcion</h1>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="PerfilRefId" class="control-label"></label>
                <select asp-for="PerfilRefId" class="form-control" asp-items="ViewBag.PerfilRefId">
                    <option value="">Elija un Jugador...</option>

                </select>
            </div>
            <div class="form-group">
                <label asp-for="TorneoRefId" class="control-label"></label>
                <select asp-for="TorneoRefId" class="form-control" asp-items="ViewBag.TorneoRefId">
                    <option value="">Elija un Torneo...</option>

                </select>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Volver a la Lista</a>
</div>

