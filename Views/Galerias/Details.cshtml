@model Identidad.Models.Galeria

@{
    ViewData["Title"] = "Details";
}
<div class="container-fluid" style="margin-left:3EM">

    <h1>Detalle Foto</h1>

    <div>
        <hr />
        <dl class="row">
            <dt class="col-sm-2 font2b">
                @Html.DisplayNameFor(model => model.FotoPath)
            </dt>
            <dd class="col-sm-10 font2">
                @Html.DisplayFor(model => model.FotoPath)
            </dd>
            <dt class="col-sm-2 font2b">
                @Html.DisplayNameFor(model => model.Fecha)
            </dt>
            <dd class="col-sm-10 font2">
                @Html.DisplayFor(model => model.Fecha)
            </dd>
            <dt class="col-sm-2 font2b">
                @Html.DisplayNameFor(model => model.Nota)
            </dt>
            <dd class="col-sm-10 font2">
                @Html.DisplayFor(model => model.Nota)
            </dd>
        </dl>
    </div>
    <div>
        <a asp-action="Edit" asp-route-id="@Model.GaleriaId">Edit</a> <br />
        <a asp-action="Index">Volver a la Lista</a>
    </div>
</div>
